            "bypass": {
                "type": "string",
                "title": "Why you bypass",
                "maxLength": 500,
                "visible": "$('1/conformity') == 'Not Conform, bypass'"
            }
        }
    },
    "data": {
        "conformity" : "Not Conform"
    }
}